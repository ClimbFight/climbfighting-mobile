{
    "name": "climbfighting_mobile",
    "version": "0.0.1",
    "private": true,
    "dependencies": {
        "mobx": "^6.3.2",
        "mobx-react-lite": "^3.2.0",
        "react": "16.13.1",
        "react-native": "0.63.3",
        "react-native-gesture-handler": "^1.8.0",
        "react-native-keyboard-aware-scroll-view": "^0.9.4",
        "react-native-reanimated": "^1.13.2",
        "react-native-screens": "^2.15.0",
        "react-navigation": "^4.4.3",
        "react-navigation-stack": "^1.10.3",
        "react-navigation-tabs": "^2.10.1",
        "styled-components": "^5.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/runtime": "^7.12.5",
        "@react-native-community/eslint-config": "^2.0.0",
        "babel-jest": "^26.6.3",
        "eslint": "^7.21.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-react-native": "^3.10.0",
        "eslint-plugin-styled-components-a11y": "0.0.32",
        "husky": "^4.2.5",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.1",
        "metro-react-native-babel-preset": "^0.64.0",
        "react-test-renderer": "16.13.1"
    },
    "scripts": {
        "lint": "eslint ./app",
        "lint:fix": "eslint --fix ./app ",
        "prettier": "prettier \"**/*.{json,yaml,md}\" --check",
        "prettier:fix": "prettier \"**/*.{json,yaml,md}\" --write"
    },
    "jest": {
        "preset": "react-native"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "airbnb"
        ]
    },
    "lint-staged": {
        "**/*.{json,yaml,md}": "prettier --check",
        "**/*.{js,jsx}": "eslint"
    },
    "husky": {
        "hooks": {
            "prepare-commit-msg": "scripts/add-issue-to-commit.sh $HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    }
}
